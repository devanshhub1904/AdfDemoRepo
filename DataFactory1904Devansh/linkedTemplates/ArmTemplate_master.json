{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "DataFactory1904Devansh"
		},
		"AzueDbPractice_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzueDbPractice'"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"LocalBlob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LocalBlob'"
		},
		"AzueDbPractice_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "dataengieer.database.windows.net"
		},
		"AzueDbPractice_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "Practice"
		},
		"AzueDbPractice_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "DataFactory1904Devansh"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakestorage1904.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "DataFactory1904Devansh_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzueDbPractice_password": {
						"value": "[parameters('AzueDbPractice_password')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"LocalBlob_connectionString": {
						"value": "[parameters('LocalBlob_connectionString')]"
					},
					"AzueDbPractice_properties_typeProperties_server": {
						"value": "[parameters('AzueDbPractice_properties_typeProperties_server')]"
					},
					"AzueDbPractice_properties_typeProperties_database": {
						"value": "[parameters('AzueDbPractice_properties_typeProperties_database')]"
					},
					"AzueDbPractice_properties_typeProperties_userName": {
						"value": "[parameters('AzueDbPractice_properties_typeProperties_userName')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "DataFactory1904Devansh_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'DataFactory1904Devansh_ArmTemplate_0')]"
			]
		}
	]
}