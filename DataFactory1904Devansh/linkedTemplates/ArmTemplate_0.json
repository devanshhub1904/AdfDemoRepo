				"typeProperties": {
					"server": "[parameters('AzueDbPractice_properties_typeProperties_server')]",
					"database": "[parameters('AzueDbPractice_properties_typeProperties_database')]",
					"encrypt": "mandatory",
					"trustServerCertificate": false,
					"authenticationType": "SQL",
					"userName": "[parameters('AzueDbPractice_properties_typeProperties_userName')]",
